<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite" thread-count="5" parallel="tests">




	<test name="Test1">
		<parameter name="os" value="Windows"></parameter>
		<parameter name="osVersion" value="10"></parameter>
		<parameter name="browser" value="Chrome"></parameter>
		<parameter name="browserVersion" value="105.0"></parameter>
		<classes>
			<class name="Day5.BrowserStack" />
		</classes>
	</test> <!-- Test -->
	
	

	<test  name="Test2">
		<parameter name="os" value="Windows"></parameter>
		<parameter name="osVersion" value="8"></parameter>
		<parameter name="browser" value="Edge"></parameter>
		<parameter name="browserVersion" value="latest"></parameter>
		<classes>
			<class name="Day5.BrowserStack" />
		</classes>
	</test> <!-- Test -->
	
	
	<test name="Test3">
		<parameter name="os" value="OS X"></parameter>
		<parameter name="osVersion" value="Sierra"></parameter>
		<parameter name="browser" value="Safari"></parameter>
		<parameter name="browserVersion" value="10.0"></parameter>
		<classes>
			<class name="Day5.BrowserStack" />
		</classes>
	</test> <!-- Test -->
	
	<test name="Test4">
		<parameter name="os" value="OS X"></parameter>
		<parameter name="osVersion" value="Sierra"></parameter>
		<parameter name="browser" value="irefox"></parameter>
		<parameter name="browserVersion" value="106.0"></parameter>
		<classes>
			<class name="Day5.BrowserStack" />
		</classes>
	</test> <!-- Test -->
	
	<test  name="Test5">
		<parameter name="os" value="Windows"></parameter>
		<parameter name="osVersion" value="11"></parameter>
		<parameter name="browser" value="Chrome"></parameter>
		<parameter name="browserVersion" value="107.0"></parameter>
		<classes>
			<class name="Day5.BrowserStack" />
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
